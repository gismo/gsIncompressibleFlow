<?xml version="1.0" encoding="UTF-8"?>

<!-- Template PETSc options for a general linear system. -->
<!-- any PETSc command-line arguments for KSP and PC can be used -->

<xml>
 <OptionList id="0">
 
  <string label="-ksp_type" desc="" value="fgmres"/>
  <string label="-ksp_initial_guess_nonzero" desc="" value="true"/>
  <string label="-ksp_max_it" desc="" value="50"/>
  <string label="-ksp_rtol" desc="" value="1e-4"/>
  
  <string label="-pc_type" desc="" value="asm"/>
  <string label="-pc_asm_overlap" desc="" value="1"/>
  <string label="-sub_pc_type" desc="" value="ilu"/>
  <string label="-sub_pc_factor_levels" desc="" value="0"/>
  <string label="-sub_pc_factor_mat_ordering_type" desc="" value="rcm"/>
  <string label="-sub_pc_factor_reuse_ordering" desc="" value=""/>

 </OptionList>
</xml>

